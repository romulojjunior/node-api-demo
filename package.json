{
  "name": "node-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node build/src/index.js",
    "dev": "npx tsc --watch",
    "watch": "nodemon",
    "lint": "yarn eslint src/",
    "test": "jest ./test/",
    "console": "ts-node ./tools/shell.ts",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate": "sequelize-cli db:migrate",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:migration:create": "sequelize-cli migration:create --name ",
    "db:model:create": "sequelize-cli model:create",
    "test:db:migrate": "export NODE_ENV=test; sequelize-cli db:migrate",
    "test:db:migrate:undo": "export NODE_ENV=test; sequelize-cli db:migrate:undo",
    "test:db:create": "export NODE_ENV=test; sequelize-cli db:create",
    "test:db:drop": "export NODE_ENV=test; sequelize-cli db:drop"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^5.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.5"
  }
}